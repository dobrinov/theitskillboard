<h3>Add new project</h3>

<%= form_for Project.new, url: profile_course_courseworks_path(course_id: target.id) do |f| %>

  <%= f.label :name, 'Name' %>
  <%= f.text_field :name %>

  <%= f.label :description, 'Description' %>
  <%= f.text_field :description %>

  <%= f.submit 'Add project', class: 'button primary' %>
<% end %>